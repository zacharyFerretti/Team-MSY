<!-- You only need 1 prodContainer to contain multiple items in a catagory-->
	<div id="prodContainer" class="prodContainer">
			<span class = "productBox">

<!-- Fill in the src as "images/[FILENAME]", ideally a square image-->
					<img class="productImage" src="images/">
		    	
<!-- Product title goes before "</div>". Don't worry about the length, it concatanates itself-->
					<div class="productName"></div>
          
<!-- Price after the $ sign-->
					<div class="productPrice">$</div>
					<div class="addToBasketButton">ADD TO BASKET</div>
			</span>	
	</div
		    
        
        
<!-- For example...-->
